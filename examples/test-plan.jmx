        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="VerifyPlanonLoginSuccessful" enabled="true">
          <stringProp name="WebDriverSampler.script">// #1
var pkg = JavaImporter(org.openqa.selenium) 
var support_ui = JavaImporter(org.openqa.selenium.support.ui.WebDriverWait)
var wait = new support_ui.WebDriverWait(WDS.browser, 60000)
// #2
WDS.sampleResult.sampleStart()
// #3 Open Cloud Environment in Chrome Browser
WDS.browser.get(&quot;https://${__P(cloudurl,)}&quot;)
java.lang.Thread.sleep(8000)
// #4 Enter Username
WDS.browser.findElement(pkg.By.id(&quot;j_username&quot;)).sendKeys(&quot;supervisor&quot;)
// #5 Enter Password
WDS.browser.findElement(pkg.By.id(&quot;j_password&quot;)).sendKeys(&quot;${__P(password,)}&quot;)
// #6 Click on Login Button
WDS.browser.findElement(pkg.By.xpath(&quot;//*[@id=&apos;loginForm&apos;]/div[4]/input&quot;)).click()
WDS.log.info(&quot;Clicked on the Login button&quot;);
// Sleep for 5 seconds
java.lang.Thread.sleep(5000)
// #7 Verify Title
if(WDS.browser.getTitle() != &apos;Planon ProCenter : Accelerator&apos;) 
{
    WDS.sampleResult.setSuccessful(false)
    WDS.sampleResult.setResponseMessage(&apos;Expected title to be Planon ProCenter : Accelerator&apos;)
}
// #8 Verify Login Successful
var result = WDS.browser.findElement(pkg.By.xpath(&quot;//*[@id=&apos;user-name&apos;]&quot;)).getText()
// #9 If Login Successful Set Teststatus to PASS else FAIL
if (result != &quot;SUPERVISOR&quot;)
 WDS.sampleResult.setSuccessful(false)
else
 WDS.sampleResult.setSuccessful(true)
// #10
WDS.sampleResult.sampleEnd()</stringProp>
